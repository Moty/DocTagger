# Example configuration for invoice processing

INBOX_FOLDER=/home/user/Documents/DocTagger/invoices-inbox
ARCHIVE_FOLDER=/home/user/Documents/DocTagger/invoices-archive

OLLAMA__MODEL=llama2
ARCHIVE_STRUCTURE={year}/{month}/Invoices

TAGS__CUSTOM_CATEGORIES=Invoice,Receipt,Bill,Statement
TAGS__MAX_TAGS=5

OCR__ENABLED=true
OCR__LANGUAGE=eng
